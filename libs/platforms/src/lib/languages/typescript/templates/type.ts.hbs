{{#ifEnum schema}}
export enum {{name}} {
  {{#each enum}}
  {{pascalCase .}} = '{{.}}',
  {{/each}}
}

{{/ifEnum}}
{{#ifObject schema}}
export type {{name}} = {
{{#ifRecord .}}
  [key: {{key}}]: {{value}};
{{/ifRecord}}
{{#typeProperty templateProperties}}
   {{name}}{{#if required}}{{else}}?{{/if}}:{{#ifRecord .}} {
    [key: {{key}}]: {{value}};
  }{{else}}{{tsType}}
  {{/ifRecord}};
{{/typeProperty}}
  _attachments?: any;
}

{{/ifObject}}

