
{{#each imports}}
import { {{pascalCase name}} } from '{{path}}';
{{/each}}

{{#each imports}}
export { {{pascalCase name}} } from '{{path}}';
{{/each}}

{{#each enums}}

export enum {{pascalCase name}} {
  {{#each properties}}
  {{name}} = '{{type}}',
  {{/each}}
}

{{/each}}

{{#each types}}

{{#if isUnion}}
export type {{pascalCase name}} = {{raw.type}}
{{else}}
export type {{pascalCase name}} = {
  {{#each properties}}
  {{name}}{{#unless required}}?{{/unless}}: {{type}};
  {{/each}}
}
{{/if}}

{{/each}}

export type Root ={{#each roots}} {{pascalCase . }}{{#unless @last}} |{{/unless}}{{/each}};
