          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/gatekeeper"],
          "options": {
            "jestConfig": "libs/gatekeeper/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
