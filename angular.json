            },
            "json": {
              "commands": [
                {
                  "command": "cd libs/core && npx compodoc -p ./tsconfig.json -e json"
                }
              ]
            },
            "json.watch": {
              "commands": [
                {
                  "command": "cd libs/core && npx compodoc -p ./tsconfig.json -e json -w"
                }
              ]
            }
          }
        }
      },
      "tags": []
    },
    "integrations": {
      "projectType": "library",
      "root": "libs/integrations",
      "sourceRoot": "libs/integrations/src",
      "prefix": "cf",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/integrations"],
          "options": {
            "jestConfig": "libs/integrations/jest.config.js",
