{
  "version": 1,
  "projects": {
    "codex": {
      "projectType": "application",
      "root": "apps/codex",
      "sourceRoot": "apps/codex/src",
      "architect": {
        "build": {
          "builder": "@nx-plus/docusaurus:browser",
          "options": {
            "outputPath": "dist/apps/codex"
          }
        },
        "serve": {
          "builder": "@nx-plus/docusaurus:dev-server",
          "options": {
            "port": 3000
          }
        }
      },
      "tags": []
    },
    "core": {
      "projectType": "library",
      "root": "libs/core",
      "sourceRoot": "libs/core/src",
      "prefix": "cf",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/core"],
          "options": {
            "jestConfig": "libs/core/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/core/src/**/*.ts",
              "libs/core/src/**/*.html"
            ]
          }
        },
        "compodoc": {
          "builder": "@nrwl/workspace:run-commands",
          "configurations": {
            "build-codex": {
              "commands": [
                {
                  "command": "cd libs/core && npx compodoc -p ./tsconfig.json -d ../../apps/codex/static/compodoc/core"
                }
              ]
            },
            "serve": {
              "commands": [
                {
                  "command": "cd libs/core && npx compodoc -p ./tsconfig.json -w"
                }
              ]
            },
            "json": {
              "commands": [
                {
                  "command": "cd libs/core && npx compodoc -p ./tsconfig.json -e json"
                }
              ]
            },
            "json.watch": {
              "commands": [
                {
                  "command": "cd libs/core && npx compodoc -p ./tsconfig.json -e json -w"
                }
              ]
            }
          }
        }
      },
      "tags": []
    },
    "gatekeeper": {
      "projectType": "library",
      "root": "libs/gatekeeper",
      "sourceRoot": "libs/gatekeeper/src",
      "prefix": "cf",
      "architect": {
